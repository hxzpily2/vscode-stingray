// A launch configuration that launches the extension inside a new window
{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": ["--extensionDevelopmentPath=${workspaceRoot}", "D:\\projects\\autodesk\\museum-of-learning-v1.6.0\\Museum of Learning"],
            "stopOnEntry": false
        }
    ]
}